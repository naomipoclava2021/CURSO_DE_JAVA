package controlador;

import javax.swing.JOptionPane;

public class EntradaSalidaDeDatoConJoptionPane {//panel de opciones

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String cadena;
		int entero;
		char letra;
		double decimal;
		
		cadena = JOptionPane.showInputDialog("Ingrese una cadena de texto: ");
		System.out.println(cadena);
		entero = JOptionPane.showInputDialog("Ingrese un entero: ");
		letra = JOptionPane.showInputDialog("Ingrese una letra: ");
		decimal = JOptionPane.showInputDialog("Ingrese un numero decimal: ");

	}

}
